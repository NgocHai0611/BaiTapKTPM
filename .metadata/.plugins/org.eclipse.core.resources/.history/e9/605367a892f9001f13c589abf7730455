package stragy;

public class ThamNienSalaryStrategy implements SalaryStrategy {
    @Override
    public int tinhLuong() {
        return 1000; // Lương cho chức vụ Thâm niên
    }
}

public class TienSiSalaryStrategy implements SalaryStrategy {
    @Override
    public int tinhLuong() {
        return 1100; // Lương cho chức vụ Tiến sĩ
    }
}

public class ToTruongSalaryStrategy implements SalaryStrategy {
    @Override
    public int tinhLuong() {
        return 1200; // Lương cho chức vụ Tổ trưởng
    }
}

public class TruongKhoaSalaryStrategy implements SalaryStrategy {
    @Override
    public int tinhLuong() {
        return 1300; // Lương cho chức vụ Trưởng khoa
    }
}
